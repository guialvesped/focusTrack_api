{
  "kind": "collectionType",
  "collectionName": "targets",
  "info": {
    "singularName": "target",
    "pluralName": "targets",
    "displayName": "target",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "target_status": {
      "type": "enumeration",
      "enum": [
        "todo",
        "doing",
        "done"
      ]
    },
    "due_date": {
      "type": "date"
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "very high",
        "high",
        "medium",
        "low"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "targets"
    },
    "todos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::todo.todo",
      "mappedBy": "target"
    }
  }
}
